<VirtualHost *:8080>
    ServerName 127.0.0.1

    WSGIScriptAlias / /home/omgtt/sites/omgtt/repository/omgtt/configs/production/production.wsgi
 
    ErrorLog /home/omgtt/logs/omgtt.apache.error.log
    LogLevel warn

    CustomLog /home/omgtt/logs/omgtt.apache.access.log combined
    
    ServerSignature Off
</VirtualHost>
